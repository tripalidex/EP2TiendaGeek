<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.carlosquispe.tiendageek1145">

    <uses-permission android:name="android.permission.INTERNET" />

    <queries>
        <intent>
            <action android:name="com.google.android.youtube.api.service.START" />
        </intent>
    </queries>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TiendaGeek1145"
        tools:targetApi="31">
        <activity
            android:name=".AudifonosActivity"
            android:exported="false" />
        <activity
            android:name=".VideojuegosActivity"
            android:exported="false"
            android:label="Videojuegos" />
        <activity
            android:name=".NoticiasActivity"
            android:exported="false"
            android:theme="@style/Theme.TiendaGeek1145.NoActionBar" />
        <activity
            android:name=".OficinasActivity"
            android:exported="false"
            android:label="@string/oficinas" />
        <activity
            android:name=".ColaboradoresActivity"
            android:exported="false"
            android:label="@string/colaboradores" />
        <activity
            android:name=".InformacionActivity"
            android:exported="false"
            android:label="@string/informacion" />
        <activity
            android:name=".AyudaActivity"
            android:exported="false"
            android:label="@string/ayuda" />
        <activity
            android:name=".PrincipalActivity"
            android:exported="false" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.TiendaGeek1145.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TerminosActivity"
            android:exported="false"
            android:label="@string/terminos" />
        <activity
            android:name=".RegistrateActivity"
            android:exported="false"
            android:label="@string/registrate" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:label="@string/login" />
        <activity
            android:name=".EmpezarActivity"
            android:exported="false"
            android:theme="@style/Theme.TiendaGeek1145.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:theme="@style/Theme.TiendaGeek1145.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>